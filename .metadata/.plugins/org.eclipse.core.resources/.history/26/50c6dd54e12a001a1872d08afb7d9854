spring:
   application:
      name: application-config-clinet
   cloud:
      config:
         name: application-config-clinet   #从github上读取的配置文件名
         profile: test #本次访问配置选项
         label: master #哪个分支
         uri: http://config2233.com:2233   #服务启动后先去2233后去github读取配置文件
      bus:
         trace:
            enabled: true     # 开启cloud bus的跟
#针对boot2开启
management:
  endpoints:
    web:
      exposure:
        include: bus-refresh   
   rabbitmq:
       addresses: localhost
       port: 5672
       username: guest
       password: guest
